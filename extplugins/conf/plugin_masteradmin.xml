<configuration plugin="masteradmin">
	<settings name="commands">
		<set name="maprestart">100</set>
		<set name="fastrestart">100</set>
		<set name="hardcore">120</set>
		<set name="gametype">100</set>
		<set name="killcam">120</set>
		<set name="ff">120</set>
		<set name="spectate">120</set>
		<set name="mag">100</set>
		<set name="setpassword">120</set>
		<set name="restorepassword">120</set>
		<set name="removepassword">80</set>
		<set name="getpassword">20</set>
